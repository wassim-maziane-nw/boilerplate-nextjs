{
  "name": "nw-nextjs-boilerplate",
  "private": true,
  "packageManager": "pnpm@10.16.1+sha512.0e155aa2629db8672b49e8475da6226aa4bdea85fdcdfdc15350874946d4f3c91faaf64cbdc4a5d1ab8002f473d5c3fcedcd197989cf0390f9badd3c04678706",
  "engines": {
    "pnpm": ">=9.0.0",
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "pnpm check-filesize && next dev --turbopack",
    "build": "next build --no-lint",
    "build:export": "NEXT_EXPORT_BUILD=true next build --no-lint",
    "build:standalone": "NEXT_STANDALONE_BUILD=true next build --no-lint",
    "start": "next start",
    "lint:target": "next lint --ignore-path .gitignore --max-warnings 0",
    "lint": "pnpm lint:target --dir .",
    "lint:all": "pnpm lint",
    "lint:fix": "pnpm lint:all --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:snap": "jest --maxWorkers=50% --silent --watchAll=false --testMatch='!**/*' --testMatch='**/*.snap.tsx'",
    "test:coverage": "jest --coverage --maxWorkers=50% --silent --watchAll=false --reporters=default --coverageReporters=cobertura --coverageReporters=text-summary --coverageReporters=json-summary --coverageReporters=lcov --testMatch='**/*.{spec,test}.{js,jsx,ts,tsx}'",
    "generate": "plop",
    "preinstall": "npx -y only-allow pnpm",
    "prepare": "next telemetry disable && node .husky/install.mjs",
    "check-filesize": "node .ci/scripts/check-filesize.js",
    "pre-push-check": "pnpm lint && pnpm test && pnpm build",
    "pre-commit-check": "pnpm typecheck && lint-staged && pnpm check-filesize && pnpm knip",
    "release": "release-it --config config/release.config.js --only-version",
    "commit-message-check": "commitlint --edit",
    "knip": "knip --config config/knip.config.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@hookform/resolvers": "^5.2.2",
    "@mui/material": "^5.10.15",
    "@reduxjs/toolkit": "^1.9.2",
    "axios": "^0.27.2",
    "i18next": "^22.4.14",
    "i18next-browser-languagedetector": "^7.0.1",
    "next": "15.4.1",
    "next-secure-headers": "^2.2.0",
    "notistack": "^2.0.8",
    "query-string": "^7.1.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.63.0",
    "react-i18next": "^12.2.0",
    "react-query": "^3.39.0",
    "react-redux": "^8.0.5",
    "sharp": "^0.33.4",
    "zod": "^4.1.9"
  },
  "prettier": {
    "endOfLine": "lf",
    "printWidth": 120,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "all"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@emotion/jest": "^11.10.5",
    "@nimbleways/eslint-config": "^0.3.0",
    "@release-it/conventional-changelog": "^10.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@total-typescript/ts-reset": "^0.6.1",
    "@tsconfig/next": "^2.0.3",
    "@tsconfig/strictest": "^2.0.5",
    "@types/eslint": "^8.4.10",
    "@types/jest": "^29.5.14",
    "@types/node": "18.11.18",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@types/react-redux": "^7.1.25",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.40.0",
    "eslint-config-next": "15.4.1",
    "glob": "^11.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "knip": "^5.45.0",
    "lint-staged": "^15.4.3",
    "next-http-proxy-middleware": "^1.2.5",
    "plop": "^3.1.1",
    "postcss": "^8.5.3",
    "prettier": "^3.0.0",
    "release-it": "^18.1.2",
    "tailwindcss": "^3.2.4",
    "typescript": "5.8.2"
  },
  "resolutions": {
    "@typescript-eslint/parser": "6.2.1",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6"
  }
}
